<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 5.2.0//EN"
	"http://www.liferay.com/dtd/liferay-service-builder_5_2_0.dtd">

<service-builder package-path="pl.mamzdanie.userorganization.svc">
	<namespace>thread_detail</namespace>
	<entity name="UserOrganization" local-service="true" remote-service="false"
		table="mz_organization">
		<column name="id" db-name="id" type="long" primary="true" />
		<column name="name" db-name="name" type="String" />
		<column name="address" db-name="address" type="String" />
		<column name="postalCode" db-name="postal_code" type="String" />
		<column name="city" db-name="city" type="String" />
		<column name="phone" db-name="phone" type="String" />
		<column name="fax" db-name="fax" type="String" />
		<column name="email" db-name="email" type="String" />
	</entity>
</service-builder>